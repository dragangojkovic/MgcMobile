<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:MBoxMobile"
             x:Class="MBoxMobile.MainPage">
  <StackLayout Padding="10">
    <Label Text="Demo MBox 2.0" VerticalOptions="Center" HorizontalOptions="Center" />
    <Button Text="Photo" Clicked="ButtonPhoto_Clicked" />
    <Button Text="BarCode Scanner" Clicked="ButtonBarcode_Clicked" />
    <Button Text="English" Clicked="ButtonEnglish_Clicked" />
    <Button Text="Srpski" Clicked="ButtonSrpski_Clicked" />
    <Button Text="User Type 1" Clicked="ButtonUserType1_Clicked" />
    <Button Text="User Type 2" Clicked="ButtonUserType2_Clicked" />
  </StackLayout>
</ContentPage>